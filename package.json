{
  "name": "wll-sdk-generator",
  "description": "A new CLI generated with oclif",
  "version": "0.0.0",
  "author": "MW & JJ",
  "bugs": "https://github.com/https://bitbucket.org/wlloyalty/wll-sdk-generator/https://bitbucket.org/wlloyalty/wll-sdk-generator/issues",
  "bin": {
    "generate": "./binz/run.js",
    "@types/bun": "latest",
    "@types/node": "^18",
    "ts-node": "^10.9.2"
  },
  "oclif": {
    "bin": "generate",
    "commands": "./src/commands",
    "dirname": "generate",
    "topicSeparator": " "
  },
  "dependencies": {
    "@oclif/core": "^4.2.10",
    "@oclif/plugin-plugins": "^5",
    "invariant": "^2.2.4",
    "json-schema-to-typescript": "^15.0.4",
    "oatyp": "^1.0.0-alpha.33",
    "ts-morph": "^11.0.3"
  },
  "devDependencies": {
    "@eslint/compat": "^1",
    "@oclif/prettier-config": "^0.2.1",
    "@oclif/test": "^4",
    "@types/bun": "^1.2.8",
    "@types/chai": "^4",
    "@types/invariant": "^2.2.37",
    "@types/mocha": "^10",
    "@types/node": "^18",
    "chai": "^4",
    "eslint": "^9",
    "eslint-config-oclif": "^6",
    "eslint-config-prettier": "^10",
    "mocha": "^10",
    "oclif": "^4",
    "openapi-types": "^12.1.3",
    "prettier": "3.5.3",
    "shx": "^0.3.3",
    "ts-node": "^10",
    "typescript": "^5"
  },
  "main": "dist/index.js",
  "type": "module",
  "repository": "https://bitbucket.org/wlloyalty/wll-sdk-generator/",
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "yarn lint",
    "prepack": "oclif manifest && oclif readme",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md",
    "dev": "bun run binz/dev.js",
    "pretty": "bun prettier . --write"
  }
}
